{
    "version": 2,
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.js"
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 */8 * * *"
        }
    ]
}
